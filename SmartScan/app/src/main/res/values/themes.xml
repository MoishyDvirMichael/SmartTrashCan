<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SmartScan" parent="Theme.AppCompat.Light.DarkActionBar">

        <!-- Original AppCompat attributes. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorSecondary">@color/colorSecondary</item>
        <item name="android:colorBackground">#f7f7f7</item>
        <item name="colorError">#B00020</item>



    </style>

    <style name="Theme.SmartScan.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="Theme.SmartScan.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.SmartScan.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>